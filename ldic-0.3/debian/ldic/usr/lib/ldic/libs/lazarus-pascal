if memo2.Lines.CommaText='yes' then	# lazarus if 문 text 리드시 작동
       cb1.Checked:=true;  # lazarus if 문 text 리드시 작동
uses Process;	 # lazarus excute program , 라자러스 bash 실행 스크립트
var	 # lazarus excute program , 라자러스 bash 실행 스크립트
     RunProgram: TProcess;	 # lazarus excute program , 라자러스 bash 실행 스크립트
RunProgram := TProcess.create(nil);  # lazarus excute program , 라자러스 bash 실행 스크립트
  RunProgram.CommandLine:= 'gedit';  # lazarus excute program , 라자러스 bash 실행 스크립트
  RunProgram.Execute; 	# lazarus excute program , 라자러스 bash 실행 스크립트
  RunProgram.Free;   # lazarus excute program , 라자러스 bash 실행 스크립트
  form1.close;		# lazarus  program exit 
 ShowMessage  ('good!')   # lazarus show dialog
if combobox1.caption='1' then	# lazarus combobox control , 라자러스 콤보박스 컨트롤 스크립트 if문
     ShowMessage  ('good!');	# lazarus combobox control , 라자러스 콤보박스 컨트롤 스크립트 if문
while <condition> do # lazarus while , 라자러스 while 반복문 , 라자러스 while 반복문
begin			# lazarus while , 라자러스 while 반복문
 the will			# lazarus while , 라자러스 while 반복문
end;				# lazarus while , 라자러스 while 반복문

