sudo add-apt-repository ppa:thefanclub/grive-tools # grive grive-tools, 구글 드라이브 싱크

sudo add-apt-repository ppa:kilian/f.lux # fluxgui *xenial :https://launchpad.net/~nathan-renniewaldock/+archive/ubuntu/flux/+build/9403904 , 리눅스 블루 라이트 차단 소프트웨어

sudo apt-add-repository ppa:darklin20/bomi  # bomi, 동영상 플레이어 보미

sudo add-apt-repository ppa:colingille/freshlight # winusb , window usb를 구워주는 소프트웨어

sudo add-apt-repository ppa:cpug-devs/ppa # cpu-g , 리눅스의 cpu-z 라고 불리우는 소프트웨어

sudo add-apt-repository ppa:maarten-baert/simplescreenrecorder #simplescreenrecorder simplescreenrecorder-lib:i386 , 화면녹화 프로그램

sudo add-apt-repository ppa:dirk-computer42/c42-backport # iceweasel , 심플 브라우저 

sudo add-apt-repository ppa:danielrichter2007/grub-customizer # grub-customizer , grub 간편 편집기 gui

sudo add-apt-repository ppa:yannubuntu/boot-repair # boot-repair, 부팅 수리 소프트웨어 

sudo add-apt-repository ppa:git-core/ppa # git-core 

sudo add-apt-repository ppa:ondrej/php5 # php5

sudo add-apt-repository ppa:nijel/phpmyadmin # phpmyadmin

sudo add-apt-repository ppa:alessandro-strada/ppa # google-drive-ocamlfus , 구글 드라이브 마운트 소프트웨어

sudo add-apt-repository ppa:paolorotolo/android-studio # android-studio , 안드로이드 스튜디오 소프트웨어

sudo add-apt-repository ppa:ubuntu-wine/ppa # wine , wine 레파시토리

sudo add-apt-repository ppa:nilarimogard/webupd8	# antimicro , 조이스틱 제어 소프트웨어 
